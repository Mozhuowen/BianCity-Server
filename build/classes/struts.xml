			<result name="success">/WEB-INF/jsp/createStory.jsp</result>
			<result name="error">/WEB-INF/jsp/error2.jsp</result>
		</action>
   	<action name="*">
			<result>/WEB-INF/jsp/{1}.jsp</result>
		</action>
	</package>
	
	
